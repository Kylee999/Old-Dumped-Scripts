--[[

local AutoCollectInfo = Tab7:CreateSection("Main Farming")


local ChristmasCollect = Tab7:CreateToggle({
  Name = "Auto Collect Presents",
  CurrentValue = false,
  Flag = "AutoCollect", 
  Callback = function(Value)
    getgenv().ChristmasCollect = Value
        while getgenv().ChristmasCollect do
        wait(0.2)
        for i,v in pairs(game:GetService("Workspace").Scripts.PresentsCollect.Storage:GetDescendants()) do
          if (v.Name == "TouchInterest" and v.Parent) then
          firetouchinterest(game.Players.LocalPlayer.Character.Head, v.Parent, 0)
          wait()
          break;
         end
        end 
        end
  end,
})

local ChristmasUpgrader = Tab7:CreateSection("Christmas Upgrades!")

function Christmasupgrade(option)

  local args = {
      [1] = option,
      [2] = "christmas"
  }
game:GetService("ReplicatedStorage").Functions.Upgrade:InvokeServer(unpack(args))
  end


local Christmasoption;
local ChristmasChoiceOption = Tab7:CreateDropdown({
  Name = "Christmas Upgrader Option",
  Options = {"Click Multiplier", "Hatch Speed", "Bank Slots", "Luck Multiplier", "Gifts Multiplier", "+1 Pet Equip"},
  CurrentOption = "Select Christmas Upgrader",
  Flag = "Christmas Upgrade",
  Callback = function(Option)
   if Option == "Click Multiplier" then
     Christmasoption = "ClickMultiplier";
   elseif Option == "Hatch Speed" then
     Christmasoption = "HatchSpeed";
  elseif Option == "Bank Slots" then
     Christmasoption = "BankSlots";
  elseif Option == "Luck Multiplier" then
     Christmasoption = "LuckMultiplier";
  elseif Option == "Gifts Multiplier" then
     Christmasoption = "GiftsMultiplier";
  elseif Option == "+1 Pet Equip" then
     Christmasoption = "PetEquip";
    end
   end       
})	


